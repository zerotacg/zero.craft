    "babel": "^5.8.24",
    "benchmark": "^2.1.0",
    "chai": "^3.4.1",
    "gh-pages": "^0.9.0",
    "jshint": "^2.8.0",
    "jspm": "^0.16.15",
    "mocha": "^2.3.4",
    "rimraf": "^2.5.1",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "jspm": {
